<script>
  {% if customer %}
    ttq.identify({
    email: '{{customer.email}}',
    phone_number: '{{customer.phone}}',
  })
  {% endif %}
    ttq.track('ViewContent')
  
  {% if template == 'product' %}
    $(document).on('submit', '.product-form--root', function(e) {
	ttq.track('AddToCart');
  });
  {% endif %}
  
  ttq.track('InitiateCheckout')
  ttq.track('AddPaymentInfo')
  
</script>
  
  
</body>

</html>