@charset "UTF-8";
/* FRAMEWORK
//////////////////////////////////////////////////////////////////////////////*/
/* Initialize
~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~*/
*,
*:before,
*:after {
  -webkit-box-sizing: border-box;
  box-sizing: border-box;
}

.clearfix:after, .fw--blocks:after {
  content: '\0020';
  display: block;
  height: 0;
  clear: both;
  overflow: hidden;
  visibility: hidden;
}

.show-after-js {
  visibility: hidden;
}

html.after-js-loaded .show-after-js {
  visibility: visible;
}

body[data-tabbing='false'] [tabindex='0'],
body[data-tabbing='false'] a,
body[data-tabbing='false'] input,
body[data-tabbing='false'] textarea,
body[data-tabbing='false'] button {
  outline: none;
}

iframe[src="//localhost:3000/preview_bar"] {
  display: none;
}

/* Loading Animation
~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~*/
.fw--loading_animation {
  display: none;
}

/* Media Queries
~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~*/
